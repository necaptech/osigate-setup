dblogin="-u$dbuser -p$dbpass $dbname"
command=$1
value1=$2
value2=$3
allValues=$@
downloadpath=/var/www/html/ownpan/download

goBack(){
echo -e "<a href=\"javascript:history.go(-1)\"><b>GoBack</b></a>"
}

